{
  "name": "@mask/tailwind-config",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "stub": "pnpm unbuild --stub"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./postcss": {
      "types": "./src/postcss.config.ts",
      "import": "./dist/postcss.config.mjs",
      "require": "./dist/postcss.config.cjs",
      "default": "./dist/postcss.config.mjs"
    },
    "./*": "./*"
  },
  "peerDependencies": {
    "tailwindcss": "^4.1.14"
  },
  "dependencies": {
    "@manypkg/get-packages": "catalog:",
    "@tailwindcss/typography": "catalog:",
    "autoprefixer": "catalog:",
    "cssnano": "catalog:",
    "postcss": "catalog:",
    "postcss-antd-fixes": "catalog:",
    "postcss-import": "catalog:",
    "postcss-preset-env": "catalog:",
    "tailwindcss": "catalog:"
  },
  "devDependencies": {
    "@types/postcss-import": "catalog:"
  }
}
